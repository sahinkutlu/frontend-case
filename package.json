{
  "name": "react-ts-vite-template",
  "description": "Boilerplate for React/Typescript, built on top of Vite ⚡️",
  "private": true,
  "version": "1.2.3",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Luis Alaguna <alagunasalahaddin@live.com>",
  "homepage": "https://github.com/SalahAdDin/react-ts-vite-template#readme",
  "repository": "git@github.com:SalahAdDin/react-ts-vite-template.git",
  "bugs": "https://github.com/SalahAdDin/react-ts-vite-template/issues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "test:preview": "vitest-preview",
    "test:e2e": "playwright test --trace on --pass-with-no-tests",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:install": "playwright install",
    "test:e2e:help": "playwright --help",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier src --check",
    "format:fix": "pnpm prettier --write",
    "format:lint": "pnpm prettier:fix && pnpm lint:fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "commitlint",
    "release": "semantic-release --dry-run --no-ci"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.1",
    "@tanstack/react-query": "4.36.1",
    "axios": "1.5.1",
    "dotenv": "16.3.1",
    "million": "2.6.4",
    "nanoid": "5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.47.0",
    "react-intersection-observer": "9.5.2",
    "valibot": "0.19.0",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@commitlint/types": "17.4.4",
    "@faker-js/faker": "8.1.0",
    "@hookform/devtools": "4.3.1",
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@playwright/test": "1.38.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.1",
    "@semantic-release/npm": "11.0.0",
    "@semantic-release/release-notes-generator": "12.0.0",
    "@tailwindcss/forms": "0.5.6",
    "@tanstack/eslint-plugin-query": "4.36.1",
    "@tanstack/react-query-devtools": "4.36.1",
    "@testing-library/dom": "9.3.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "20.8.4",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.16.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.0.2",
    "eslint-plugin-vitest": "0.3.2",
    "happy-dom": "12.9.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "msw": "1.3.2",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.5",
    "semantic-release": "22.0.5",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vitest": "0.34.6",
    "vitest-dom": "0.1.1",
    "vitest-preview": "0.0.1",
    "web-vitals": "3.5.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "keywords": ["react", "boilerplate", "vite", "bundle", "typescript"]
}
